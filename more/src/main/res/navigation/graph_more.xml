<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/graph_more"
  app:startDestination="@id/moreFragment">

  <fragment
    android:id="@+id/moreFragment"
    android:name="com.idiot.more.ui.MoreFragment"
    android:label="MoreFragment" >
    <action
      android:id="@+id/action_moreFragment_to_registerFragment"
      app:destination="@id/registerFragment" />
  </fragment>

  <fragment
    android:id="@+id/registerFragment"
    android:name="com.idiot.more.ui.RegisterFragment"
    android:label="RegisterFragment" >
    <deepLink app:uri="estate://register_fragment"/>
    <action
      android:id="@+id/action_registerFragment_to_registerInfoFragment"
      app:destination="@id/registerInfoFragment" />
  </fragment>
  <fragment
    android:id="@+id/registerInfoFragment"
    android:name="com.idiot.more.ui.RegisterInfoFragment"
    android:label="RegisterInfoFragment" >
    <argument
      android:name="estateId"
      app:argType="long"/>
    <action
      android:id="@+id/action_registerInfoFragment_to_registerHouseInfoFragment"
      app:destination="@id/registerHouseInfoFragment" />
    <action
      android:id="@+id/action_registerInfoFragment_to_addressSearchDialogFragment"
      app:destination="@id/addressSearchDialogFragment" />
  </fragment>
  <dialog
    android:id="@+id/registerHouseInfoFragment"
    android:name="com.idiot.more.ui.RegisterHouseInfoFragment"
    android:label="RegisterHouseInfoFragment" />
  <dialog
    android:id="@+id/addressSearchDialogFragment"
    android:name="com.idiot.more.ui.AddressSearchDialogFragment"
    android:label="AddressSearchDialogFragment" />

</navigation>
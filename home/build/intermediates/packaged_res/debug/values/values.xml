    <style name="Base.Theme.EStateTwin" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        
        <item name="colorPrimary">@color/purple_700</item>
        <item name="colorPrimaryVariant">@color/purple_500</item>
        <item name="colorOnPrimary">@color/white</item>
        
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        
        
        
        <item name="android:colorBackground">@color/white</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
        
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="windowActionBarOverlay">true</item>
        
        <item name="android:textViewStyle">@style/PretendardTextView</item>
        <item name="android:buttonStyle">@style/PretendardButton</item>
    </style>
    <style name="PretendardButton" parent="TextAppearance.AppCompat.Widget.Button">
        <item name="fontFamily">@font/pretendard</item>
    </style>
    <style name="PretendardTextView" parent="android:Widget.TextView">
        <item name="fontFamily">@font/pretendard</item>
        <item name="android:includeFontPadding">false</item>
    </style>
    <style name="ShapeAppearance.Estate.Card" parent="ShapeAppearance.MaterialComponents">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">4dp</item>
    </style>
    <style name="TextAppearance.idiot.ActionBar.Title" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">17sp</item>
        <item name="android:fontFamily">@font/pretendard_regular</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="TextAppearance.idiot.Appbar.Text" parent="TextAppearance.AppCompat.Large">
        <item name="android:fontFamily">@font/pretendard_bold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:lineSpacingExtra">22sp</item>
    </style>
    <style name="TextAppearance.idiot.icon.Text" parent="TextAppearance.AppCompat.Medium">
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">@font/pretendard</item>
    </style>
    <style name="TextAppearance.idiot.info.content.Text" parent="TextAppearance.AppCompat.Medium">
        <item name="fontFamily">@font/ff_notosanskr_regular</item>
        <item name="android:textColor">@color/detail_info_content</item>
        <item name="android:textSize">13sp</item>
    </style>
    <style name="TextAppearance.idiot.info.subTitle.Text" parent="TextAppearance.AppCompat.Medium">
        <item name="android:fontFamily">@font/notosanskr_medium</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">13sp</item>
    </style>
    <style name="TextAppearance.idiot.sub.Text" parent="TextAppearance.AppCompat.Small">
        <item name="android:fontFamily">@font/ff_notosanskr_regular</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/gray_500</item>
        <item name="android:textSize">13sp</item>
    </style>
    <style name="TextAppearance.idiot.tab.Text" parent="TextAppearance.AppCompat.Medium">
        <item name="android:fontFamily">@font/notosanskr_medium</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/tab_inactivated</item>
    </style>
    <style name="TextAppearance.idiot.tab.title.Text" parent="TextAppearance.AppCompat.Medium">
        <item name="android:fontFamily">@font/notosanskr_medium</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">13sp</item>
    </style>
    <style name="TextAppearance.idiot.title.Text" parent="TextAppearance.AppCompat.Medium">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">@font/pretendard</item>
    </style>
    <style name="Theme.idiot.BottomSheet.Rounded" parent="Theme.MaterialComponents.Light.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/bottomSheetStyleWrapper</item>
    </style>
    <style name="Widget.idiot.TabLayout" parent="Widget.MaterialComponents.TabLayout">
        <item name="tabTextAppearance">@style/TextAppearance.idiot.tab.Text</item>
    </style>
    <style name="bottomSheetStyleWrapper" parent="Widget.Design.BottomSheet.Modal">
        <item name="android:background">@android:color/transparent</item>
    </style>
</resources>
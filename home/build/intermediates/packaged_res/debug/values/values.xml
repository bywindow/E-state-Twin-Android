    <string name="app_name">E-State Twin</string>
    <string name="appbar_search">매물 검색하기</string>
    <string name="hello_blank_fragment">Hello blank fragment</string>
    <string name="home">홈</string>
    <string name="home_recommend_card_image_desc">Picture of recommended house</string>
    <string name="main_appbar_help">내가 찾고있는 집을\n빠르게 검색해보세요!</string>

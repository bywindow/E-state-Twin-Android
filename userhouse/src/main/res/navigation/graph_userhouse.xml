<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/graph_userhouse"
  app:startDestination="@id/myHouseFragment">

  <fragment
    android:id="@+id/myHouseFragment"
    android:name="com.idiot.userhouse.ui.MyHouseFragment"
    android:label="MyHouseFragment" >
    <action
      android:id="@+id/action_myHouseFragment_to_manageAssetFragment"
      app:destination="@id/manageAssetFragment" />
    <action
      android:id="@+id/action_myHouseFragment_to_ownerChecklistPagerFragment"
      app:destination="@id/ownerChecklistPagerFragment" />
  </fragment>
  <fragment
    android:id="@+id/manageAssetFragment"
    android:name="com.idiot.userhouse.ui.ManageAssetFragment"
    android:label="fragment_manage_asset"
    tools:layout="@layout/fragment_manage_asset">
    <argument app:argType="string" android:name="category"/>
  </fragment>
  <fragment
    android:id="@+id/tenantModeFragment"
    android:name="com.idiot.userhouse.ui.TenantModeFragment"
    android:label="TenantModeFragment" >
  </fragment>
  <fragment
    android:id="@+id/ownerChecklistPagerFragment"
    android:name="com.idiot.userhouse.ui.OwnerChecklistPagerFragment"
    android:label="fragment_owner_checklist_pager"
    tools:layout="@layout/fragment_owner_checklist_pager">
    <argument app:argType="long" android:name="estateId"/>
  </fragment>

</navigation>
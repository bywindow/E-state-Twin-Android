<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.idiot.common_ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_houseDetailFragment"
            app:destination="@id/houseDetailFragment" />
    </fragment>

    <fragment
        android:id="@+id/interestFragment"
        android:name="com.idiot.common_ui.interest.InterestFragment"
        android:label="fragment_interest"
        tools:layout="@layout/fragment_interest" />

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.idiot.common_ui.search.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/myHouseFragment"
        android:name="com.idiot.common_ui.myhouse.MyHouseFragment"
        android:label="fragment_my_house"
        tools:layout="@layout/fragment_my_house" />

    <fragment
        android:id="@+id/moreFragment"
        android:name="com.idiot.common_ui.mypage.MoreFragment"
        android:label="fragment_more"
        tools:layout="@layout/fragment_more" >
        <action
            android:id="@+id/action_moreFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/houseDetailFragment"
        android:name="com.idiot.common_ui.detail.HouseDetailFragment"
        android:label="HouseDetailFragment">
        <argument
            android:name="houseId"
            app:argType="long"/>
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.idiot.common_ui.register.RegisterFragment"
        android:label="RegisterFragment" >
        <action
            android:id="@+id/action_registerFragment_to_registerInfoFragment"
            app:destination="@id/registerInfoFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerInfoFragment"
        android:name="com.idiot.common_ui.register.RegisterInfoFragment"
        android:label="RegisterInfoFragment">
        <argument
            android:name="estateId"
            app:argType="long"/>
        <action
            android:id="@+id/action_registerInfoFragment_to_addressSearchDialogFragment"
            app:destination="@id/addressSearchDialogFragment" />
    </fragment>
    <fragment
        android:id="@+id/addressSearchDialogFragment"
        android:name="com.idiot.common_ui.register.AddressSearchDialogFragment"
        android:label="AddressSearchDialogFragment" />

</navigation>